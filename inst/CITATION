citHeader("To cite package 'helloWorld' in publications use:")

citEntry(entry = "Manual",
         title        = "{helloWorld}: ",
         author       = "Damian W. Betebenner",
         year         = "2014",
         note         = "R package version 1.0",
         url          = "https://github.com/dbetebenner/helloWorld",
         
         textVersion  = paste("Damian W. Betebenner (2014).",
                        "helloWorld: A sleek and modern hello world function.",
                        "(R package version 1.0",
                        "URL https://github.com/dbetebenner/helloWorld")
)
